<h1>Songs:</h1>

<table>
    <tr>    
        <th>Title</th>
        <th>Release Year</th>
        <th>Genre</th>
        <th>Artist</th>
    </tr>
    <tbody>
    <% @songs.each do |song|%>
        <tr>
            <td><%= song.title %></td>
            <td><%= song.release_year %></td>
            <td><%= song.genre %></td>      
            <td><%= song.artist_name %></td>
            <td>
                <%= link_to 'View Song', song %>
                <%= link_to 'Edit Song', edit_song_path(song) %>
                <%= link_to 'Delete', song, method: :delete, data: {confirm: 'Are You Sure?' } %>
            </td>
        </tr>
    <%end%>
</tbody>



</table>